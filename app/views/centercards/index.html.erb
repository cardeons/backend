<p id="notice"><%= notice %></p>

<h1>Centercards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @centercards.each do |centercard| %>
      <tr>
        <td><%= link_to 'Show', centercard %></td>
        <td><%= link_to 'Edit', edit_centercard_path(centercard) %></td>
        <td><%= link_to 'Destroy', centercard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Centercard', new_centercard_path %>
